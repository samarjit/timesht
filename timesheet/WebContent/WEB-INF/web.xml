	<servlet>
		<description>
		</description>
		<display-name>
		WorkflowController</display-name>
		<servlet-name>WorkflowController</servlet-name>
		<servlet-class>workflow.WorkflowController</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>
		WorkflowSearch</display-name>
		<servlet-name>WorkflowSearch</servlet-name>
		<servlet-class>
		workflow.WorkflowSearch</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
