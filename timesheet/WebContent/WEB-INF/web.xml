	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>
		FlowControllerServlet</display-name>
		<servlet-name>FlowControllerServlet</servlet-name>
		<servlet-class>
		workflow.FlowControllerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WorkflowController</servlet-name>
		<url-pattern>/WorkflowController.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>WorkflowSearch</servlet-name>
		<url-pattern>/WorkflowSearch.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ScreenFlowControllerServlet</servlet-name>
		<url-pattern>/ScreenFlowControllerServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
